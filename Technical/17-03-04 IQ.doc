\{\
    // B declares A as a friend...\
    friend class A;\
 \
private:\
    void privatePrint()\
    \{\
        std::cout << "hello, world" << std::endl;\
    \}\
\};\
 \
class A\
\{\
public:\
    A()\
    \{\
        B b;\
        // ... and A now has access to B's private members\
        b.privatePrint();\
    \}\
\};\
 \
int main()\
\{\
    A a;\
    return 0;\
\}\
