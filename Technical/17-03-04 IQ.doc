\par \hich\af4\dbch\af31505\loch\f4         //Perform increment operation
\par \hich\af4\dbch\af31505\loch\f4         return temp;
\par \hich\af4\dbch\af31505\loch\f4     \}
\par \}\hich\af4\dbch\af31505\loch\f4 ;
